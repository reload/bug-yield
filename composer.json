{
  "name": "reload/bugyield",
  "description": "A script to sync between Harvest and Jira.",
  "license": "MIT",
  "autoload" : {
    "psr-4": {
      "BugYield\\": "src/"
    }
  },
  "platform": {
    "php": "7.1",
    "ext-curl": "*",
    "ext-xml": "*",
    "ext-mbstring": "*"
  },
  "require": {
    "php": ">=7.2.0",
    "gridonic/hapi": "dev-master",
    "symfony/console": "^5",
    "symfony/yaml": "^5",
    "alexkovalevych/jira-api": ">=1.0.6",
    "mnapoli/silly-php-di": ">=1.2",
    "sendgrid/sendgrid": "^7.8"
  },
  "require-dev": {
    "phpspec/phpspec": "^6",
    "bossa/phpspec2-expect": "^3"
  }
}
